<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="org.m2ling.domain.core.presentation.CoreModelWizardID"
            name="%_UI_CoreModelWizard_label"
            class="org.m2ling.domain.core.presentation.CoreModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/CoreModelFile.gif">
         <description>%_UI_CoreModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <editor
            id="org.m2ling.domain.core.presentation.CoreEditorID"
            name="%_UI_CoreEditor_label"
            icon="icons/full/obj16/CoreModelFile.gif"
            extensions="core"
            class="org.m2ling.domain.core.presentation.CoreEditor"
            contributorClass="org.m2ling.domain.core.presentation.CoreActionBarContributor">
      </editor>
   </extension>

   <extension point="org.eclipse.ui.newWizards">
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="org.m2ling.domain.parameters.presentation.ParametersModelWizardID"
            name="%_UI_ParametersModelWizard_label"
            class="org.m2ling.domain.parameters.presentation.ParametersModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/ParametersModelFile.gif">
         <description>%_UI_ParametersModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <editor
            id="org.m2ling.domain.parameters.presentation.ParametersEditorID"
            name="%_UI_ParametersEditor_label"
            icon="icons/full/obj16/ParametersModelFile.gif"
            extensions="parameters"
            class="org.m2ling.domain.parameters.presentation.ParametersEditor"
            contributorClass="org.m2ling.domain.parameters.presentation.ParametersActionBarContributor">
      </editor>
   </extension>

   <extension point="org.eclipse.ui.newWizards">
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="org.m2ling.domain.principles.presentation.PrinciplesModelWizardID"
            name="%_UI_PrinciplesModelWizard_label"
            class="org.m2ling.domain.principles.presentation.PrinciplesModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/PrinciplesModelFile.gif">
         <description>%_UI_PrinciplesModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <editor
            id="org.m2ling.domain.principles.presentation.PrinciplesEditorID"
            name="%_UI_PrinciplesEditor_label"
            icon="icons/full/obj16/PrinciplesModelFile.gif"
            extensions="principles"
            class="org.m2ling.domain.principles.presentation.PrinciplesEditor"
            contributorClass="org.m2ling.domain.principles.presentation.PrinciplesActionBarContributor">
      </editor>
   </extension>

</plugin>
